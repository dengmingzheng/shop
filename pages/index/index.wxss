.swiper-group{
  width: 100%;
}

.swiper-group .swioer-item image{
  width: 100%;
  height: 100%;
}

.index_cates{
  display: flex;
}

.index_cates navigator{
  padding: 20rpx;
  flex:1;
}

.index_cates navigator image{
   width: 100%;
   height: 100%;
}

.index_floor .floor_group .floor_title{
     padding: 10rpx 0;
}

.index_floor .floor_group .floor_title image{
  width: 100%;
  height: 100%;
}

.index_floor .floor_group .floor_list{
  /* 清除浮动 */
  overflow: hidden;
}

.index_floor .floor_group .floor_list navigator{
  float: left;
  width: 33.33%;

}

.index_floor .floor_group .floor_list navigator:nth-last-child(-n+4){
  /* height: 33.33vw * 386 / 232 / 2; */
  height: 27.72711207vw;
  border-left: 10rpx solid #ffffff;
}

.index_floor .floor_group .floor_list navigator:nth-of-type(2){
  border-bottom: 10rpx solid #ffffff;
}

.index_floor .floor_group .floor_list navigator:nth-of-type(3){
  border-bottom: 10rpx solid #ffffff;
}

.index_floor .floor_group .floor_list navigator image{
  width: 100%;
  height: 100%;
}